<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectejosepcliment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Menu_inicial"
            android:label="@string/app_name"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu_inicial" />
        </activity>

        <activity
            android:name=".Calculadora"
            android:label="@string/app_name"
            android:parentActivityName=".Menu_inicial" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Calculadora" />
        </activity>
        <activity
        android:name=".Informacion"
        android:label="@string/app_name"
        android:parentActivityName=".Menu_inicial" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".Informacion" />
    </activity>

        <activity
            android:name=".PrincipalClassRecycler"
            android:label="@string/app_name"
            android:parentActivityName=".Menu_inicial" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PrincipalClassRecycler" />
        </activity>


    </application>
</manifest>
